#include <bits/stdc++.h>
using namespace std;

int main() {

	// your code goes here
	long t;
	cin>>t;
	while(t--)
	{
	    long n; 
	    cin>>n;
	    int ans=0;
	    int i=5;
	    while(n/i>=1)
	    {
	        ans+=n/i;
	        i*=5;
	        
	    }
	    cout<<ans;
	    cout<<"\n";
	}
	return 0;
}
